diff -ruN binutils-2.35/ld/configure.tgt binutils/ld/configure.tgt
--- binutils-2.35/ld/configure.tgt	2020-07-24 11:12:20.000000000 +0200
+++ binutils/ld/configure.tgt	2020-12-23 19:35:02.106412526 +0100
@@ -43,6 +43,15 @@
 # Please try to keep this table more or less in alphabetic order - it
 # makes it much easier to lookup a specific archictecture.
 case "${targ}" in
+i[3-7]86-*-muos*)
+			targ_emul=elf_i386_muos
+			targ_extra_emuls=elf_i386
+			targ64_extra_emuls="elf_x86_64_muos elf_x86_64"
+			;;
+x86_64-*-muos*)
+			targ_emul=elf_x86_64_muos
+			targ_extra_emuls="elf_i386_muos elf_x86_64 elf_i386"
+			;;
 aarch64_be-*-elf)	targ_emul=aarch64elfb
 			targ_extra_emuls="aarch64elf aarch64elf32 aarch64elf32b armelfb armelf"
 			;;
