diff -ruN binutils-2.35/bfd/config.bfd binutils/bfd/config.bfd
--- binutils-2.35/bfd/config.bfd	2020-07-24 11:12:19.000000000 +0200
+++ binutils/bfd/config.bfd	2020-12-23 19:37:16.467842761 +0100
@@ -222,6 +222,19 @@
 
 case "${targ}" in
 # START OF targmatch.h
+  i[3-7]86-*-muos*)
+    targ_defvec=i386_elf32_vec
+    targ_selvecs=
+    targ64_selvecs=x86_64_elf64_vec
+    ;;
+    
+#ifdef BFD64
+  x86_64-*-muos*)
+    targ_defvec=x86_64_elf64_vec
+    targ_selvecs=i386_elf32_vec
+    want64=true
+    ;;
+#endif
 #ifdef BFD64
   aarch64-*-darwin*)
     targ_defvec=aarch64_mach_o_vec
