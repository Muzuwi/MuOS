diff -ruN gcc-10.2.0/gcc/config.gcc gcc/gcc/config.gcc
--- gcc-10.2.0/gcc/config.gcc	2020-07-23 08:35:17.316384243 +0200
+++ gcc/gcc/config.gcc	2020-12-23 20:20:07.888194085 +0100
@@ -675,6 +675,12 @@
 
 # Common parts for widely ported systems.
 case ${target} in
+*-*-muos*)
+  gas=yes
+  gnu_ld=yes
+  default_use_cxa_atexit=yes
+  use_gcc_stdint=wrap
+  ;;
 *-*-darwin*)
   tmake_file="t-darwin "
   tm_file="${tm_file} darwin.h"
@@ -1033,6 +1039,12 @@
 esac
 
 case ${target} in
+i[34567]86-*-muos*)
+    tm_file="${tm_file} i386/unix.h i386/att.h dbxelf.h elfos.h glibc-stdint.h i386/i386elf.h muos.h"
+    ;;
+x86_64-*-muos*)
+    tm_file="${tm_file} i386/unix.h i386/att.h dbxelf.h elfos.h glibc-stdint.h i386/i386elf.h i386/x86-64.h muos.h"
+    ;;
 aarch64*-*-elf | aarch64*-*-fuchsia* | aarch64*-*-rtems*)
 	tm_file="${tm_file} dbxelf.h elfos.h newlib-stdint.h"
 	tm_file="${tm_file} aarch64/aarch64-elf.h aarch64/aarch64-errata.h aarch64/aarch64-elf-raw.h"
